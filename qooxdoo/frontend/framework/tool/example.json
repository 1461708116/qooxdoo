{
  "common" :
  {
    "library" :
    [
      {
        "encoding" : "utf-8",
        "path" : "framework/source/class",
        "uri" : "../source/class"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/apiviewer/source/class",
        "uri" : "../source/class"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/feedreader/source/class",
        "uri" : "../source/class"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/webmail/source/class",
        "uri" : "../source/class"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/showcase/source/class",
        "uri" : "../source/class"
      }
    ],

    "require" :
    {
      "qx.log.Logger" : ["qx.log.appender.Native"]
    },

    "cache" :
    {
      "path" : "cache2"
    }
  },




  "api-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },

  "api-webmail" :
  {
    "extend" : ["common"],
    "include" : ["webmail.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },




  "source-all" :
  {
    "extend" : ["common"],

    "source" :
    {
      "file" : "source-all.js"
    }
  },

  "build-all" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-all.js"
    }
  },




  "source-apiviewer" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "source" :
    {
      "file" : "apiviewer-source.js"
    }
  },

  "build-apiviewer" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "compile" :
    {
      "file" : "apiviewer-build.js",
      "optimize" : [ "variables", "basecalls" ]
    }
  },

  "build-apiviewer-variants" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "compile" :
    {
      "file" : "apiviewer-variants-build-$variant.js",
      "optimize" : [ "variables", "basecalls" ]
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.aspects" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    }
  },

  "source-apiviewer-parts" :
  {
    "extend" : ["common"],

    "source" :
    {
      "file" : "source-apiviewer-pkgs-$variant-$package.js"
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "optimize" : 5000,

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
      }
    }
  },

  "build-apiviewer-parts" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-apiviewer-pkgs-$variant-$package.js"
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "optimize" : 5000,

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
      }
    }
  },

  "build-apiviewer-parts-noqx" :
  {
    "extend" : ["build-parts-common"],

    "compile" :
    {
      "file" : "build-apiviewer-pkgs-noqx-$variant-$package.js"
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "include" : ["apiviewer.Application"],
      "exclude" : ["=qx.*"],

      "parts" :
      {
        "items" :
        {
          "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
          "viewer" : ["apiviewer.Viewer"],
          "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
        }
      }
    }
  }
}
